{
    "version": "0.2.0",
    "configurations": [
		{
			"name": "Local invoke",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/local-invoke.js",
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
				"AWS_PROFILE": "bensmith",
				"NODE_LOCAL": "true" // enable console.logs & file save
            }
		},
		{
			"name": "Local analyse",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/local-analyser.js",
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
				"AWS_PROFILE": "bensmith",
				"NODE_LOCAL": "true"
            }
		},
		{
			"name": "Node selected file",
			"type": "node",
			"request": "launch",
			"program": "${file}",
			"env": {
				"AWS_PROFILE": "bensmith",
				"NODE_LOCAL": "true"
			}
		},
    ]
}